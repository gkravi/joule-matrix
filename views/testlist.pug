extends base

block content
    main.main        
        h1.heading-primary.full-width.purple-background.heading-page= `${enroll.environment} Tests`
        .website-report.container-xl
            .approve-all__tests.clearfix
                button.btn.btn--green.btn--approve-all.float-right(data-testId=`${enroll.slug}`, data-tenant=`${enroll.tenant}`) Approve All
            nav.test-list__wrapper
                #nav-tab.nav.nav-tabs(role='tablist')
                    a#nav-imagediff-tab.nav-item.nav-link.active(data-toggle='tab' href='#nav-imagediff' role='tab' aria-controls='nav-imagediff' aria-selected='true') ImageDiff
                    a#nav-htmldiff-tab.nav-item.nav-link(data-toggle='tab' href='#nav-htmldiff' role='tab' aria-controls='nav-htmldiff' aria-selected='false') HtmlDiff
                    a#nav-speedtest-tab.nav-item.nav-link(data-toggle='tab' href='#nav-speedtest' role='tab' aria-controls='nav-speedtest' aria-selected='false') SpeedTest
                #nav-tabContent.tab-content
                    #nav-imagediff.tab-pane.fade.show.active(role='tabpanel' aria-labelledby='nav-imagediff-tab')
                        each test in enroll.tests
                            - if (test.type.toLowerCase() === 'imagediff')
                                                                
                                .row.enroll-result.report--item.list-group-item
                                    .col-md-2.my-auto
                                        a.btn.btn-outline-info.btn-outline__test--details(href=`/test/${test.slug}`)
