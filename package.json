		"pino": "^6.5.1",
		"pino-http": "^5.2.0",
		"pixelmatch": "^5.2.0",
		"pug": "^3.0.0",
		"puppeteer": "^3.3.0",
		"pusher": "^3.0.1",
		"saml2js": "^0.1.2",
		"sanitize-html": "^1.27.0",
		"sharp": "^0.25.4",
		"slugify": "^1.4.0",
		"stripe": "^8.60.0",
		"swagger-jsdoc": "^4.0.0",
		"swagger-ui-express": "^4.1.4",
		"text-diff": "^1.0.1",
		"useragent": "^2.3.0",
		"validator": "^13.1.1",
		"xss-clean": "^0.1.1"
	},
	"devDependencies": {
		"@types/jest": "^25.2.3",
		"@types/jest-cli": "^24.3.0",
		"eslint": "^7.2.0",
		"eslint-config-airbnb": "^18.1.0",
		"eslint-config-prettier": "^6.11.0",
