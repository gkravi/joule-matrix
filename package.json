{
	"name": "visual-testing",
	"version": "1.0.0",
	"description": "Joule Visual Regression Testing",
	"main": "app.js",
	"scripts": {
		"start": "node server.js | pino-pretty",
		"dev": "nodemon server.js | pino-pretty > joulvt-dev.log 2> error-dev.log",
		"start:prod": "NODE_ENV=production nodemon --max-old-space-size=2048 server.js | pino-pretty",
		"test-snapshots": "jest",
		"test-snapshots-update": "jest --config=jest.config.js --updateSnapshot --detectOpenHandles -- forceExit",
		"test:percy": "percy exec -- jest -i ./tests-snapshots/__tests__/percy.test.js",
		"start:dev": "nodemon server.js",
		"debug": "ndb server.js",
		"watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js",
		"build:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js"
	},
	"keywords": [
		"test",
		"puppeteer",
		"visual",
		"testing"
	],
	"author": "Ravi Gupta",
	"license": "ISC",
	"dependencies": {
		"@babel/polyfill": "^7.10.4",
		"@percy/puppeteer": "^1.1.0",
		"amqplib": "^0.6.0",
		"async": "^3.2.0",
		"aws-sdk": "^2.709.0",
		"axios": "^0.19.2",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.5.0",
		"chai": "^4.2.0",
		"compression": "^1.7.4",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-mongo-sanitize": "^2.0.0",
		"express-rate-limit": "^5.1.3",
		"helmet": "^3.23.2",
		"hpp": "^0.2.3",
		"html-to-text": "^5.1.1",
		"jest": "^26.0.1",
		"jest-cli": "^26.0.1",
		"jquery": "^3.5.1",
		"jsonwebtoken": "^8.5.1",
		"mocha": "^7.2.0",
		"mongoose": "^5.9.19",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"multer-s3": "^2.9.0",
		"nodemailer": "^6.4.10",
		"passport": "^0.4.1",
